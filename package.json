{
  "name": "@bpanel/publish-data",
  "version": "0.0.1",
  "description": "A simple bPanel plugin for publishing data on the blockchain using OP_RETURN",
  "main": "dist/index.js",
  "scripts": {
    "babel": "babel --presets=env,stage-3 lib -d dist",
    "watch": "babel --presets=env,stage-3 lib -d dist --watch",
    "clean": "rm -f ./dist/*.js",
    "lint": "eslint lib/ test/ || exit 0",
    "prepublish": "make babel",
